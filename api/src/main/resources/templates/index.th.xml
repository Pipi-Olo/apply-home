<?xml version="1.0"?>
<thlogic>
    <attr sel="tr[0]" th:each="home : ${homes}">
        <attr sel="td.houseName" th:text="${home.houseName}" />
        <attr sel="td.constructionCompany" th:text="${home.constructionCompany}" />
        <attr sel="td.region" th:text="${home.region}" />
        <attr sel="td.subscriptionType" th:text="${home.subscriptionType}" />
        <attr sel="td.houseType" th:text="${home.houseType}" />
        <attr sel="td.recruitmentDay" th:text="${home.recruitmentDay}" />
        <attr sel="td.subscriptionStartDay" th:text="${home.subscriptionStartDay}" />
        <attr sel="td.subscriptionEndDay" th:text="${home.subscriptionEndDay}" />
        <attr sel="td.announcementDay" th:text="${home.announcementDay}" />
    </attr>
</thlogic>